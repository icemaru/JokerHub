local ChatTab = Window:MakeTab({
    Name = "Chat Spam",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

local ChatBox = ""

ChatTab:AddTextbox({
    Name = "メッセージ入力",
    Default = "",
    TextDisappear = false,
    Callback = function(value)
        ChatBox = value
    end
})

ChatTab:AddButton({
    Name = "Send Chat",
    Callback = function()
        if ChatBox ~= "" then
            game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(ChatBox, "All")
        end
    end
})

OrionLib:Init()
